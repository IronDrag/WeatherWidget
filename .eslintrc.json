{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "class-methods-use-this" : "off",
        "max-len": ["error", { 
            "code": 150,
            "ignoreUrls": true,
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true
            }],
        "no-unused-vars" : "off",
        "no-underscore-dangle" : "off"
    }
}
